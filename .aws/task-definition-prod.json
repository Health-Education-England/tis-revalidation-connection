{
  "containerDefinitions": [
    {
      "name": "tis-revalidation-connection",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/tis-revalidation-connection:latest",
      "portMappings": [
        {
          "containerPort": 8090
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "tis-revalidation-connection"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        },
        {
          "name": "SENTRY_ENVIRONMENT",
          "value": "prod"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-prod-tis-revalidation-connection",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "awslogs-tis-revalidation-connection"
        }
      },
      "secrets": [
        {
          "name": "MONGODB_HOST",
          "valueFrom": "/tis/revalidation/prod/db/host"
        },
        {
          "name": "MONGODB_PORT",
          "valueFrom": "/tis/revalidation/prod/db/port"
        },
        {
          "name": "MONGODB_USERNAME",
          "valueFrom": "/tis/revalidation/prod/db/username"
        },
        {
          "name": "MONGODB_PASSWORD",
          "valueFrom": "/tis/revalidation/prod/db/password"
        },
        {
          "name": "RABBITMQ_HOST",
          "valueFrom": "tis-revalidation-prod-private-lb-url"
        },
        {
          "name": "RABBITMQ_PASSWORD",
          "valueFrom": "tis-revalidation-rabbit-prod-password"
        },
        {
          "name": "RABBITMQ_PORT",
          "valueFrom": "tis-revalidation-rabbit-prod-port"
        },
        {
          "name": "RABBITMQ_USERNAME",
          "valueFrom": "tis-revalidation-rabbit-prod-username"
        },
        {
          "name": "RABBITMQ_USE_SSL",
          "valueFrom": "tis-revalidation-rabbit-prod-use-ssl"
        },
        {
          "name": "GMC_CONNECT_URL",
          "valueFrom": "tis-revalidation-gmc-prod-connect-url"
        },
        {
          "name": "GMC_USER_NAME",
          "valueFrom": "tis-revalidation-gmc-prod-username"
        },
        {
          "name": "GMC_PASSWORD",
          "valueFrom": "tis-revalidation-gmc-prod-password"
        },
        {
          "name": "SENTRY_DSN",
          "valueFrom": "tis-revalidation-connection-sentry-dsn"
        },
        {
          "name": "ES_URLS",
          "valueFrom": "es7-4prod-url"
        }
      ]
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "tis-revalidation-connection-prod",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
